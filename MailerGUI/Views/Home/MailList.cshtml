@model List<MailerGUI.Models.appmail>
@{
    ViewBag.Title = "MailList";
}

<h2>MailList</h2>

<table class="table table-bordered table-condensed table-striped" >
    <thead>
        <tr class="active">
            <td>ID</td>
            <td>Subject</td>
            <td>Retry</td>
            <td>Status</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var y in Model)
        {
            WriteLiteral("<tr>");
            WriteLiteral("<td>" + y.id + "</td>");
            WriteLiteral("<td>" + y.Subject + "</td>");
            WriteLiteral("<td>" + y.Retries + "</td>");
            WriteLiteral("<td>" + y.isSent + "</td>");
            WriteLiteral("<td>" + Html.ActionLink("View", "MailDetails/"+y.id,"Home") + "</td>");
            WriteLiteral("</tr>");

        }
    </tbody>
</table>
   
